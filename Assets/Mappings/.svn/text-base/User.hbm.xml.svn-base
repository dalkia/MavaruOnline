<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="mavaruonline" namespace="Entidades">

  <class name="User">
  
  <id name="Id" column ="IdUser" type="int" unsaved-value="0">
      <generator class="identity"/>
    </id>

    <property name="Username" type="string"/>
